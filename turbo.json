{
 "$schema": "https://turborepo.com/schema.json",
 "ui": "tui",
 "globalEnv": [
  "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
  "NEXT_PUBLIC_BASE_URL",
  "NEXT_PUBLIC_AUTH_URL",
  "NEXT_PUBLIC_CLIENT_URL",
  "NEXT_PUBLIC_ADMIN_URL",
  "CLERK_SECRET_KEY",
  "DATABASE_URL",
  "NODE_ENV",
  "GCP_PROJECT_ID",
  "GCP_CLIENT_EMAIL",
  "GCP_PRIVATE_KEY",
  "GCP_BUCKET_NAME",
  "UPSTASH_REDIS_REST_URL",
  "UPSTASH_REDIS_REST_TOKEN",
  "REVALIDATION_SECRET",
  "BETTER_AUTH_SECRET",
  "ADMIN_WEBHOOK_SECRET"
 ],
 "globalDependencies": [".env"],
 "tasks": {
  "build": {
   "dependsOn": ["^build"],
   "inputs": ["$TURBO_DEFAULT$", ".env*"],
   "outputs": [".next/**", "!.next/cache/**", "dist/**"]
  },
  "lint": {
   "dependsOn": ["^lint"]
  },
  "check-types": {
   "dependsOn": ["^check-types"]
  },
  "dev": {
   "cache": false,
   "persistent": true,
   "inputs": [
    "$TURBO_DEFAULT$",
    ".env.development.local",
    ".env.local",
    ".env.development",
    ".env"
   ]
  },
  "start": {
   "cache": false,
   "persistent": true,
   "inputs": [
    "$TURBO_DEFAULT$",
    ".env.development.local",
    ".env.local",
    ".env.development",
    ".env"
   ]
  }
 }
}
